{
	"name": "hl-tutor",
	"version": "0.0.1",
	"type": "module",
	"private": true,
	"packageManager": "pnpm@10.0.0",
	"scripts": {
		"postinstall": "cp ./node_modules/z3-solver/build/z3-built.* ./node_modules/coi-serviceworker/coi-serviceworker.js static/",
		"dev": "vite dev",
		"build": "svelte-kit sync && vite build && pnpm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepare": "lezer-generator src/lib/lang-support/lezer-imp/imp.grammar -o src/lib/lang-support/lezer-imp/lezer-imp.js",
		"prepublishOnly": "pnpm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest --disable-console-intercept",
		"lint": "prettier --check .",
		"format": "prettier --write src/**/*.{css,html,ts,svelte}"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@huggingface/prettier-plugin-vertical-align": "^0.2.3",
		"@lezer/generator": "^1.7.2",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.16.1",
		"@sveltejs/package": "^2.3.9",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@types/lodash": "^4.17.14",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.19.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^5.2.3",
		"eslint-plugin-svelte": "^2.46.1",
		"gh-pages": "^6.3.0",
		"jest": "^29.7.0",
		"postcss": "^8.5.1",
		"postcss-import": "^16.1.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.5.14",
		"publint": "^0.2.12",
		"rehype-katex-svelte": "^1.2.0",
		"remark-math": "^2.0.1",
		"svelte": "5.19.0",
		"svelte-check": "^4.1.4",
		"svelte-feather-icons": "^4.2.0",
		"tailwindcss": "^3.4.17",
		"tslib": "^2.8.1",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.21.0",
		"vite": "^6.0.11",
		"vitest": "^3.0.4"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"dependencies": {
		"@codemirror/autocomplete": "^6.18.4",
		"@codemirror/commands": "^6.8.0",
		"@codemirror/language": "^6.10.8",
		"@codemirror/lint": "^6.8.4",
		"@codemirror/search": "^6.5.8",
		"@codemirror/state": "^6.5.1",
		"@codemirror/view": "^6.36.2",
		"@lezer/common": "^1.2.3",
		"@lezer/highlight": "^1.2.1",
		"@lezer/lr": "^1.4.2",
		"@types/codemirror": "^5.60.15",
		"@types/common-tags": "^1.8.4",
		"bits-ui": "1.0.0-next.78",
		"clsx": "^2.1.1",
		"codemirror": "^6.0.1",
		"coi-serviceworker": "^0.1.7",
		"common-tags": "^1.8.2",
		"crelt": "^1.0.6",
		"csstype": "^3.1.3",
		"katex": "^0.16.21",
		"lodash": "^4.17.21",
		"rehype-katex": "^7.0.1",
		"rehype-mathjax": "^6.0.0",
		"runed": "^0.23.1",
		"tailwind-merge": "^2.6.0",
		"tailwind-variants": "^0.2.1",
		"ts-pattern": "^5.6.2",
		"ts-unimplemented": "^2.0.0",
		"tslog": "^4.9.3",
		"z3-solver": "^4.13.4"
	}
}
